    <script src="~/lib/jvectorworldmap/src/abstract-shape-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-group-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-canvas-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-shape-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-path-element.js"></script>
    <script src="~/lib/jvectorworldmap/src/svg-circle-element.js"></script>
