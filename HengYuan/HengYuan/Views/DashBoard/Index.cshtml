<script>
    $(function () {

       var array = [];
       @foreach (var item in Model) {
       @:array.push({ latLng: [@item.Latitude, @item.Longtitude], name: '@item.City' });
    }

        var map = new jvm.Map({
            container: $('.map'),
            map: 'world_mill_en',
            markerStyle: {
                initial: {
                    fill: '#F8E23B',
                    stroke: '#383f47'
                }
            },
            backgroundColor: '#383f47',
            markers: array
        });

    });
</script>

